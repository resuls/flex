# Copy this file to .env.production and fill in your values

# Database Configuration
DATABASE_URL="file:./dev.db"

# Hostaway API Configuration (Required)
HOSTAWAY_API_KEY="your_production_hostaway_api_key"
HOSTAWAY_ACCOUNT_ID="your_hostaway_account_id"
HOSTAWAY_BASE_URL="https://api.hostaway.com/v1"

# Google APIs Configuration (Optional)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY="your_production_google_maps_api_key"
GOOGLE_PLACES_API_KEY="your_production_google_places_api_key"

# Application Configuration
NODE_ENV="production"
USE_MOCK_DATA="false"
